  /* 2 */
}

.header-tools:after, .product-images:after, .product:after, .subfooter:after, .blog-sidebar:after,
.blog-promo:after, .blog-wrapper:after, .template-article .main-content:after, .blog-wrapper-masonry:after, .rte:after {
